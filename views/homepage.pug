doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        a(href="/update-cobj") Add to this table
        table
            thead
                tr
                    th Name
                    th Class
                    th Number of Legs
            tbody 
                each animal in data
                    tr 
                        td #{animal.properties.name}
                        td #{animal.properties.class}
                        td #{animal.properties.number_of_legs}